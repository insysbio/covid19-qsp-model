  {
    "class": "Const",
    "id": "NA_pmole",
    "title": "Avogadro constant for 1 pmole",
    "tags": [
      "AUX"
    ],
    "units": "item/pmole",
    "num": 602000000000
  },
  {
    "class": "Const",
    "id": "k_apo_pc",
    "title": "rate constant of PC and iPC apoptosis",
    "notes": "the value was estimated from the HS submodel at steady state. Indeed, taking into account that (i) PC2 is precursors of PC1 [9227501], (ii) life span of PC1 is about 120 days [20616357], (iii) ratio of PC1/PC2 at steady state = 19/37 [7103258] we represented lung cell  dynamics system as follows: --(Vol_alv*k_mat_pc) --> PC2 --(Vol_alv*k_dif_pc*PC2)--> PC1--(Vol_alv*k_apo_pc1*PC1)-->. Then consider the system at steady state and assuming that lifespan (T_ls) corresponds to time required for 99% of cell die we have come to following formulas: (a) k_apo_pc1 = ln(1/0.01)/(120 days) = 1.6e-3 1/h, (b) k_dif_pc = k_apo_pc1*PC1_ss/PC2_ss = 8.2e-4 1/h, (c) k_mat_pc = k_dif_pc*PC2_ss. Since  PC2 to PC1 transition was not considered in the version of the model we can assume that k_apo_pc = k_dif_pc",
    "tags": [
      "PC"
    ],
    "units": "1/h",
    "num": 0.00082
  },
  {
    "class": "Const",
    "id": "k_apo_vpc",
    "title": "rate constant of vPC apoptosis",
    "notes": "the value was estimated from [PMID 18487354] only",
    "tags": [
      "PC"
    ],
    "units": "1/h",
    "num": 0.00082
  },
  {
    "class": "Const",
    "id": "kbase_tran_pc_ipc",
    "title": "rate constant of PC to iPC transition",
    "notes": "fitted to describe in vivo viral load dynamics",
    "tags": [
      "PC"
    ],
    "units": "1/pM/h",
    "num": 0.01
  },
  {
    "class": "Const",
    "id": "kbase_tran_ipc_vpc",
    "title": "rate constant of iPC to vPC transition",
    "notes": "fitted to describe in vivo viral load dynamics",
    "tags": [
      "PC"
    ],
    "units": "1/pM/h",
    "num": 0.01
  },
  {
    "class": "Const",
    "id": "k_shed_ace2_pc",
    "title": "rate constant of ACE2 shedding",
    "notes": "rate constant of shedding catalysed by ADAM17/TACE = 5e-4 1/s",
    "tags": [
      "COV-PC"
    ],
    "units": "1/h",
    "num": 1.8
  },
  {
    "class": "Const",
    "id": "k_off_cov_ace2",
    "title": "rate constant of COV_ACE2 complex dissociation",
    "notes": "k_off for viral Spike receptor binding domain (RBD) and ACE2 receptor",
    "tags": [
      "COV-PC"
    ],
    "units": "1/h",
    "num": 23.56
  },
  {
    "class": "Const",
    "id": "Kd_cov_ace2",
    "title": "equilibrium dissociation constant of COV to ACE2 binding",
    "notes": "Kd for viral Spike receptor binding domain (RBD) and ACE2 receptor is equal to 4.67 nM. Basing on estimate that virion includes about 150 spike proteins (virion diameter=120-160 nm, Spike height=20nm) we have calculated Kd for COV and ACE2 binding equal to 4.67 nM/150 = 31.13 pM",
    "tags": [
      "COV-PC"
    ],
    "units": "pM",
    "num": 31.13
  },
  {
    "class": "Const",
    "id": "k_ent_cov_ipc",
    "title": "rate constant of COV entry in iPC cell",
    "notes": "estimated on the basis of rate constant of endocytosis for Influenza virus",
    "tags": [
      "COV-iPC"
    ],
    "units": "1/h",
    "num": 0.936
  },
  {
    "class": "Const",
    "id": "k_rel_cov_ipc",
    "title": "rate constant of COV release to bulk phase from iPC and vPC cell",
    "notes": "fitted to describe in vivo viral load dynamics averaged across 9 patients",
    "tags": [
      "COV-iPC"
    ],
    "units": "1/h",
    "num": 0.6
  },
  {
    "class": "Const",
    "id": "k_unc_cov_vpc",
    "title": "rate constant of COV uncouting in vPC cell",
    "notes": "estimated on the basis of rate constant of endocytosis for Influenza virus: k_end-cyt_Vend in original paper describing release from endosome/uncoating",
    "tags": [
      "COV-vPC"
    ],
    "units": "1/h",
    "num": 14
  },
  {
    "class": "Const",
    "id": "k_rep_cov_rna_vpc",
    "title": "rate constant of COV_RNA replication in vPC cell",
    "notes": "fitted to describe in vivo viral load dynamics averaged across 9 patients",
    "tags": [
      "COV-vPC"
    ],
    "units": "1/h",
    "num": 0.6
  },
  {
    "class": "Const",
    "id": "EC50_rep_cov_rna_vpc",
    "title": "EC50 of COV_RNA replication referring to concentration inindividual vPC cell",
    "notes": "it was proposed that a host cell can produce about 8000 virions of Influenza virus before it dies. So we have calculated the EC50 basing on the value.",
    "tags": [
      "COV-vPC"
    ],
    "units": "pM",
    "num": 10000
  },
  {
    "class": "Const",
    "id": "k_ass_cov_vpc",
    "title": "rate constant of COV assembling in vPC",
    "notes": "fitted to describe in vivo viral load dynamics averaged across 9 patients",
    "tags": [
      "COV-vPC"
    ],
    "units": "1/h",
    "num": 1
  },
  {
    "class": "Const",
    "id": "k_deg_cov",
    "title": "rate constant of COV degradation in bulk phase",
    "notes": "COV half-life on different surfaces and aerosole is varied from 1 to 8 hours. We have chosen half-life equal to 4 hour. So, rate constant is equal to ln2/4 = 0.17 1/h",
    "tags": [
      "COV-vPC"
    ],
    "units": "1/h",
    "num": 0.17
  },
  {
    "class": "Const",
    "id": "Nmax_cov_per_cell",
    "title": "maximal number of viral particles able to be simultaneously attached to pneumocyte",
    "notes": "maximal number of virions Nmax = (surface area of PC faced to alveoli lumen)/(area of \"maximal\" section of SARS-CoV-2). Based on PC surface area = 183 um^2 [7103258] and (virion diameter=120-160 nm), one obtains Nmax =  183 um^2 / Pi*(140/2 nm) ^2 = 11900",
    "tags": [
      "COV-PC"
    ],
    "units": "item/cell",
    "num": 11900
  },
  {
    "class": "Const",
    "id": "k_deg_cov_rna",
    "title": "rate constant of COV_RNA degradation in bulk phase",
    "notes": "assumed to be equal to COV half-life equal to 4 hour. So, rate constant is equal to ln2/4 = 0.17 1/h",
    "tags": [
      "COV-vPC"
    ],
    "units": "1/h",
    "num": 0.17
  },
